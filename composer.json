{
    "name":        "netcommons/install",
    "description": "Install Plugin for NetCommons3",
    "type":        "cakephp-plugin",
    "keywords":    ["cakephp", "install"],
    "homepage":    "http://www.netcommons.org/",
    "license":     "NetCommons License",
    "installors": [
        {
            "name": "NetCommons Community",
            "homepage": "http://www.netcommons.org"
        }
    ],
    "extra": {
        "installer-paths": {
            "app/Plugin/{$name}": ["type:cakephp-plugin"]
        }
    },
    "require": {
        "cakedc/migrations": "~2.2",
        "netcommons/access-counters": "@stable",
        "netcommons/announcements": "@stable",
        "netcommons/auth-general": "@stable",
        "netcommons/bbses": "@stable",
        "netcommons/blogs": "@stable",
        "netcommons/cabinets": "@stable",
        "netcommons/calendars": "@stable",
        "netcommons/circular-notices": "@stable",
        "netcommons/faqs": "@stable",
        "netcommons/iframes": "@stable",
        "netcommons/links": "@stable",
        "netcommons/menus": "@stable",
        "netcommons/net-commons": "@stable",
        "netcommons/photo-albums": "@stable",
        "netcommons/plugin-manager": "@stable",
        "netcommons/questionnaires": "@stable",
        "netcommons/quizzes": "@stable",
        "netcommons/registrations": "@stable",
        "netcommons/rooms": "@stable",
        "netcommons/rss-readers": "@stable",
        "netcommons/searches": "@stable",
        "netcommons/site-manager": "@stable",
        "netcommons/system-manager": "@stable",
        "netcommons/tasks": "@stable",
        "netcommons/topics": "@stable",
        "netcommons/user-attributes": "@stable",
        "netcommons/user-manager": "@stable",
        "netcommons/user-roles": "@stable",
        "netcommons/videos": "@stable"
    },
    "config": {
        "vendor-dir": "vendors",
        "process-timeout": 3000
    }
}
