{
    "name":        "netcommons/install",
    "description": "Install Plugin for NetCommons3",
    "type":        "cakephp-plugin",
    "keywords":    ["cakephp", "install"],
    "homepage":    "http://www.netcommons.org/",
    "license":     "NetCommons License",
    "installors": [
        {
            "name": "NetCommons Community",
            "homepage": "http://www.netcommons.org"
        }
    ],
    "extra": {
        "installer-paths": {
            "app/Plugin/{$name}": ["type:cakephp-plugin"]
        }
    },
    "require": {
        "cakedc/migrations": "~2.2",
        "netcommons/access-counters": "dev-master",
        "netcommons/announcements": "dev-master",
        "netcommons/auth-general": "dev-master",
        "netcommons/bbses": "dev-master",
        "netcommons/blogs": "dev-master",
        "netcommons/cabinets": "dev-master",
        "netcommons/calendars": "dev-master",
        "netcommons/circular-notices": "dev-master",
        "netcommons/faqs": "dev-master",
        "netcommons/iframes": "dev-master",
        "netcommons/links": "dev-master",
        "netcommons/menus": "dev-master",
        "netcommons/net-commons": "dev-master",
        "netcommons/photo-albums": "dev-master",
        "netcommons/plugin-manager": "dev-master",
        "netcommons/questionnaires": "dev-master",
        "netcommons/quizzes": "dev-master",
        "netcommons/registrations": "dev-master",
        "netcommons/rooms": "dev-master",
        "netcommons/rss-readers": "dev-master",
        "netcommons/searches": "dev-master",
        "netcommons/site-manager": "dev-master",
        "netcommons/system-manager": "dev-master",
        "netcommons/topics": "dev-master",
        "netcommons/user-attributes": "dev-master",
        "netcommons/user-manager": "dev-master",
        "netcommons/user-roles": "dev-master",
        "netcommons/videos": "dev-master"
    },
    "config": {
        "vendor-dir": "vendors",
        "process-timeout": 3000
    }
}
